    $(document).ready(function()
    {
        fillHeaders();
        fillTargetOsBar();

        $('#targetOsRow').show();
        clearTargetArch();
    });
    
    function fillHeaders()
    {
        if (announcement.length > 0)
        {
            $('#announcementBar').html(announcement);
            $('#announcementBar').show();
        }
        
        $('#documentationLink').html(documentation);
        $('#releaseNotesLink').html(releaseNotes);
        $('#eulaLink').html(eula);
        $('#checksumLink').html(checksum);
        $('#quickStartGuideLink').html(quickStartGuide);
        $('#quickStartGuideLink').html(quickStartGuide);
        $('#tooltipTargetArchitecture').attr('title', 'The architecture is the CPU type of the platform.\n     "x86_64" is for 64-bit Intel x86 (also referred as "amd64").\n     "x86" is for 32-bit Intel x86 architectures (also referred as "i386" or "i686").\n     "armv7" is the 32-bit ARMv7 architecture.\n     "aarch64" is for the 64-bit ARMv8 architecture.\n     "ppc64le" is for the IBM P8 Little Endian architecture.');
        $('#tooltipSelectTargetPlatform').attr('title', 'The target is the platform on which you execute the application.\nIt may be different from the host platform, which is the platform that you will be using to edit and build an application.\nFor instance, if you edit, build, and run the application on the same Linux x86_64 machine, your host platform and your target platform are the same.\nIf you edit and build on a Linux x86_64 machine and execute the application on a Linux armv7 board, your host platform is Linux x86_64 and your target platform is Linux armv7.');
        $('#tooltipTargetInstallerType').attr('title', 'The same installer may be offered in 2 versions: local and network.\nThe local installer is self-contained and includes every component. It is a large file that only needs to be downloaded from the internet once and can be installed on multiple systems. It is the recommended type of installer with low-bandwidth internet connections.\nThe network installer is a small installer client that will only download the required components during the installation. It is faster to download but requires re-downloading each component with each new installation.'); 
        $('#tooltipCrossPrompt').attr('title', 'For some platforms (the target platform), it is possible to develop and build from a different platform (the host platform).\nThe target platform is the platform where your application will run.\nIf your host platform and target platform are not the same, answer yes to this question.'); 
        $('#tooltipSelectHostPlatform').attr('title', 'The host platform is the platform that you will be using to edit and build an application.\nIt may be different from the target platform, which is the platform on which you execute the application.\nFor instance, if you edit, build, and run the application on the same Linux x86_64 machine, your host platform and your target platform are the same.\nIf you edit and build on a Linux x86_64 machine and execute the application on a Linux armv7 board, your host platform is Linux x86_64 and your target platform is Linux armv7.'); 
        $('#tooltipHostArchitecture').attr('title', 'The architecture is the CPU type of the platform.\n     "x86_64" is for 64-bit Intel x86 (also referred as "amd64").\n     "x86" is for 32-bit Intel x86 architectures (also referred as "i386" or "i686").\n     "armv7" is the 32-bit ARMv7 architecture.\n     "aarch64" is for the 64-bit ARMv8 architecture.\n     "ppc64le" is for the IBM P8 Little Endian architecture.'); 
        $('#tooltipHostInstallerType').attr('title', 'The same installer may be offered in 2 versions: local and network.\nThe local installer is self-contained and includes every component. It is a large file that only needs to be downloaded from the internet once and can be installed on multiple systems. It is the recommended type of installer with low-bandwidth internet connections.\nThe network installer is a small installer client that will only download the required components during the installation. It is faster to download but requires re-downloading each component with each new installation.'); 
    }

    function fillTargetOsBar()
    {
        var html = '';
        for (var ii = 0; ii < targetOses.length; ii++)
        {
            var os = targetOses[ii];
            html += '<button id="target_' + os + '" class="btn btn-success btn-sm targetOsButton cdButton">' + os + '</button>';
        }

        $('#targetOsSelections').html(html);

        setupButtonHighlighting('.targetOsButton');

        $('.targetOsButton').click(function()
        {
            clearTargetArch();
            var button = $(this);
            targetOS = button.attr('id').replace(/target_/g, '');
            fillTargetArchBar();
        });
    }

    function fillHostOsBar()
    {
        var html = '';
        for (var ii = 0; ii < hostOses.length; ii++)
        {
            var os = hostOses[ii];
            html += '<button id="host_' + os + '" class="btn btn-success btn-sm hostOsButton cdButton">' + os + '</button>';
        }

        $('#hostOsSelections').html(html);

        setupButtonHighlighting('.hostOsButton');

        $('.hostOsButton').click(function()
        {
            clearHostArch();
            var button = $(this);
            hostOS = button.attr('id').replace(/host_/g, '');;
            fillHostArchBar();
        });

        if (hostOses.length == 1)
            $('#host_' + hostOses[0]).click();
    }

    function fillTargetArchBar()
    {
        var arches = targetArchMap[targetOS];

        var html = '';
        for (var ii = 0; ii < arches.length; ii++)
        {
            var arch = arches[ii];
            html += '<button id="target_' + arch + '" class="btn btn-success btn-sm targetArchButton cdButton">' + arch + '</button>';
        }

        $('#targetArchSelections').html(html);
        $('#targetArchRow').show();

        setupButtonHighlighting('.targetArchButton');

        $('.targetArchButton').click(function()
        {
            clearTargetDistro();

            var button = $(this);
            targetArch = button.attr('id').replace(/target_/g, '');

            if (targetOS.indexOf('Windows') >= 0)
            {
                var targetVersionsWindows = osArchVersionMap[targetOS + '/' + targetArch]; 
                fillTargetVersionBar(targetVersionsWindows);
            }
            else if (targetOS.indexOf('Linux') >= 0)
                fillTargetDistroBar();
            else if (targetOS.indexOf('Mac OSX') >= 0)
            {
                var targetVersionsMac = osArchVersionMap[targetOS + '/' + targetArch]; 
                fillTargetVersionBar(targetVersionsMac);
            }
        });

        if (arches.length == 1)
            $('#target_' + arches[0]).click();
    }

    function fillHostArchBar()
    {
        var arches = hostArchMap[hostOS];

        var html = '';
        for (var ii = 0; ii < arches.length; ii++)
        {
            var arch = arches[ii];
            html += '<button id="host_' + arch + '" class="btn btn-success btn-sm hostArchButton cdButton">' + arch + '</button>';
        }

        $('#hostArchSelections').html(html);
        $('#hostArchRow').show();

        setupButtonHighlighting('.hostArchButton');

        $('.hostArchButton').click(function()
        {
            clearHostDistro();

            var button = $(this);
            hostArch = button.attr('id').replace(/host_/g, '');

            if (hostOS.indexOf('Windows') >= 0)
                fillHostVersionBar(hostVersionsWindows);
            else if (hostOS.indexOf('Linux') >= 0)
                fillHostDistroBar();
            else if (hostOS.indexOf('Mac OSX') >= 0)
                fillHostVersionBar(hostVersionsMacOSX);
        });

        if (arches.length == 1)
            $('#host_' + arches[0]).click();
    }

    function fillTargetDistroBar()
    {
        var linuxDistros = targetLinuxArchToDistroMap[targetArch];
        var html = '';
        for (var ii = 0; ii < linuxDistros.length; ii++)
        {
            var distro = linuxDistros[ii];
            html += '<button id="target_' + distro + '" class="btn btn-success btn-sm targetDistroButton cdButton">' + distro + '</button>';
        }

        $('#targetDistroSelections').html(html);
        $('#targetDistroRow').show();

        setupButtonHighlighting('.targetDistroButton');

        $('.targetDistroButton').click(function()
        {
            clearTargetVersion();
            var button = $(this);
            targetOS = 'Linux';
            targetDistro = button.attr('id').replace(/target_/g, '');
            var key = targetArch + '/' + targetDistro;
            var versions = targetLinuxDistroToVersionMap[key];
            fillTargetVersionBar(versions);
            $('#targetVersionRow').show();
        });

        if (linuxDistros.length == 1)
            $('#target_' + linuxDistros[0]).click();
    }

    function fillHostDistroBar()
    {
        var linuxDistros = hostLinuxArchToDistroMap[hostArch];
        var html = '';
        for (var ii = 0; ii < linuxDistros.length; ii++)
        {
            var distro = linuxDistros[ii];
            html += '<button id="host_' + distro + '" class="btn btn-success btn-sm hostDistroButton cdButton">' + distro + '</button>';
        }

        $('#hostDistroSelections').html(html);
        $('#hostDistroRow').show();

        setupButtonHighlighting('.hostDistroButton');

        $('.hostDistroButton').click(function()
        {
            clearHostVersion();
            var button = $(this);
            hostOS = 'Linux';
            hostDistro = button.attr('id').replace(/host_/g, '');
            var key = hostArch + '/' + hostDistro;
            var versions = hostLinuxDistroToVersionMap[key];
            fillHostVersionBar(versions);
            $('#hostVersionRow').show();
        });

        if (linuxDistros.length == 1)
            $('#host_' + linuxDistros[0]).click();
    }
    
    function fillTargetVersionBar(listOfOsVersions)
    {
        var jQueryWorthySelectors = [];
        var html = '';
        for (var ii = 0; ii < listOfOsVersions.length; ii++)
        {
            var osVersion = listOfOsVersions[ii];
            var osVersionJqueryWorthy = makeSelectorJqueryWorthy(osVersion);
            html += '<button id="target_' + osVersionJqueryWorthy + '" name="' + osVersion + '" class="btn btn-success btn-sm targetVersionButton cdButton">' + osVersion + '</button>';
            jQueryWorthySelectors.push(osVersionJqueryWorthy);
        }

        $('#targetVersionSelections').html(html);
        $('#targetVersionRow').show();

        setupButtonHighlighting('.targetVersionButton');

        $('.targetVersionButton').click(function()
        {
            clearTargetPackage();
            var button = $(this);
            targetVersion = button.attr('name');
            var key = targetOS + '/' + targetArch + '/' + targetDistro + '/' + targetVersion;
            var packages = targetVersionToPackageMap[key];
            fillTargetInstallerTypeBar(packages);
        });

        if (jQueryWorthySelectors.length == 1)
            $('#target_' + jQueryWorthySelectors[0]).click();
    }

    function fillHostVersionBar(listOfOsVersions)
    {
        var jQueryWorthySelectors = [];
        var html = '';
        for (var ii = 0; ii < listOfOsVersions.length; ii++)
        {
            var osVersion = listOfOsVersions[ii];
            var osVersionJqueryWorthy = makeSelectorJqueryWorthy(osVersion);
            html += '<button id="host_' + osVersionJqueryWorthy + '" name="' + osVersion + '" class="btn btn-success btn-sm hostVersionButton cdButton">' + osVersion + '</button>';
            jQueryWorthySelectors.push(osVersionJqueryWorthy);
        }

        $('#hostVersionSelections').html(html);
        $('#hostVersionRow').show();

        setupButtonHighlighting('.hostVersionButton');

        $('.hostVersionButton').click(function()
        {
            clearHostPackage();
            var button = $(this);
            hostVersion = button.attr('name');
            var key = hostOS + '/' + hostArch + '/' + hostDistro + '/' + hostVersion;
            var packages = hostVersionToPackageMap[key];
            fillHostInstallerTypeBar(packages);
        });

        if (jQueryWorthySelectors.length == 1)
            $('#host_' + jQueryWorthySelectors[0]).click();
    }

    function fillTargetInstallerTypeBar(packages)
    {
        var jQueryWorthySelectors = [];
        var html = '';
        for (var ii = 0; ii < packages.length; ii++)
        {
            var downloadPackageType = packages[ii];
            var jQueryWorthySelector = makeSelectorJqueryWorthy(downloadPackageType);
            html += '<button id="target_' + jQueryWorthySelector + '" name="' + downloadPackageType + '" class="btn btn-success btn-sm targetInstallerTypeButton cdButton">' + downloadPackageType + '</button>';
            jQueryWorthySelectors.push(jQueryWorthySelector);
        }

        $('#targetPackageSelections').html(html);
        $('#targetPackageRow').show();

        setupButtonHighlighting('.targetInstallerTypeButton');

        $('.targetInstallerTypeButton').click(function()
        {
            var button = $(this);
            targetPackage = button.attr('name');
            clearCrossPrompt();
            handleCrossButtons();
        });

        if (jQueryWorthySelectors.length == 1)
            $('#target_' + jQueryWorthySelectors[0]).click();
    }

    function fillHostInstallerTypeBar(packages)
    {
        var jQueryWorthySelectors = [];
        var html = '';
        for (var ii = 0; ii < packages.length; ii++)
        {
            var downloadPackageType = packages[ii];
            var jQueryWorthySelector = makeSelectorJqueryWorthy(downloadPackageType);
            html += '<button id="host_' + jQueryWorthySelector + '" name="' + downloadPackageType + '" class="btn btn-success btn-sm hostInstallerTypeButton cdButton">' + downloadPackageType + '</button>';
            jQueryWorthySelectors.push(jQueryWorthySelector);
        }

        $('#hostPackageSelections').html(html);
        $('#hostPackageRow').show();

        setupButtonHighlighting('.hostInstallerTypeButton');

        $('.hostInstallerTypeButton').click(function()
        {
            var button = $(this);
            hostPackage = button.attr('name');
            showDownloadPanels();
        });

        if (jQueryWorthySelectors.length == 1)
            $('#host_' + jQueryWorthySelectors[0]).click();
    }

    function shouldShowPrompt()
    {
        var key = targetOS + '/' + targetArch + '/' + targetDistro + '/' + targetVersion + '/' + targetPackage;
        var shouldShowPrompt = versionToHostsMap[key] != null ? true : false;

        return shouldShowPrompt;
    }

    function handleCrossButtons()
    {
        var bShowPrompt = shouldShowPrompt();
        setupButtonHighlighting('.crossButton');

        if (bShowPrompt)
        {
            $('#crossPrompt').show();
            setupButtonHighlighting('.crossButton');

            $('.crossButton').click(function()
            {
                clearHostPanel();
                
                var crossCompileButton = $(this);
                var crossAnswer = crossCompileButton.attr('id');

                if (crossAnswer.indexOf('crossYes') >= 0)
                    showHostPanel();
                else if (crossAnswer.indexOf('crossNo') >= 0)
                    showDownloadPanels();
            });
        }
        else
        {
            showDownloadPanels();
        }
    }
    
    function showHostPanel()
    {
        clearHostArch();
        fillHostOsBar();

        $('#selectHostPlatformPanel').show();
    }
    
    function showDownloadPanels()
    {
        showDownloadPanel();
        showInstructionsPanels();
    }
    
    function showDownloadPanel()
    {
        $('#downloadPanel').show();
        $('#panelHostInstaller').hide();
        $('#panelTargetInstaller').hide();
        
        {
            var key = targetOS + '/' + targetArch + '/' + targetDistro + '/' + targetVersion + '/' + targetPackage;
            var targetInstallerSize = versionMap[key][INDEX_SIZE];
            var targetMd5String = versionMap[key][INDEX_MD5];
            var targetUrl = versionMap[key][INDEX_INSTALLER_URL];
            var targetFileName = targetUrl.substr(targetUrl.lastIndexOf('/') + 1);
            var targetGpuDevKitName = versionMap[key][INDEX_GPU_DEV_KIT_NAME];
            var targetGpuDevKitURL = versionMap[key][INDEX_GPU_DEV_KIT_URL];
            var targetGpuDevKitText = 'The ' + targetGpuDevKitName + ' is available as a separate download <a href="' + targetGpuDevKitURL + '">here</a>.<br/><br/>';
            var targetGsgName = versionMap[key][INDEX_GSG_NAME];
            var targetGsgURL = versionMap[key][INDEX_GSG_URL];
            var targetQsgName = versionMap[key][INDEX_QSG_NAME];
            var targetQsgURL = versionMap[key][INDEX_QSG_URL];

            var config = targetOS + ' ' + targetArch;

            if (targetDistro.length > 0)
                config += ' ' + targetDistro;
            
            config += ' ' + targetVersion;

            // title
            {
                var targetInstallerTitle = targetOS;

                if (targetDistro.length > 0)
                    targetInstallerTitle += ' ' + targetDistro;
                    
                targetInstallerTitle +=  ' ' + targetVersion + ' ' + targetArch;

                $('#titleTargetInstaller').html('Download Target Installer for ' + targetInstallerTitle);                
            }
            
            $('#targetInstallerFile').html(targetFileName);
            $('#targetInstallerSize').html(targetInstallerSize);
            $('#targetInstallerConfig').html(config);
            $('#targetMd5Checksum').html(targetMd5String);
            $('#targetDownloadButtonText').html('Download (' + targetInstallerSize + ')');
            $('#targetDownloadButtonHref').attr('href', targetUrl);
            $('#targetGSG').attr('href', targetGsgURL);
            $('#targetGSG').html(targetGsgName);
            $('#targetQSG').attr('href', targetQsgURL);
            $('#targetQSG').html(targetQsgName);
            $('#downloadPanelTitle').html('Download Installer');

            if (targetGpuDevKitURL.length > 0)
               $('#targetGpuDevKitText').html(targetGpuDevKitText);
            else
               $('#targetGpuDevKitText').empty();

            $('#panelTargetInstaller').show();
        }
        
        {
            var key = hostOS + '/' + hostArch + '/' + hostDistro + '/' + hostVersion + '/' + hostPackage;
            var entry = versionMap[key];
            
            if (entry != null)
            {
                var hostInstallerSize = versionMap[key][INDEX_SIZE];
                var hostMd5String = versionMap[key][INDEX_MD5];
                var hostUrl = versionMap[key][INDEX_INSTALLER_URL];
                var hostFileName = hostUrl.substr(hostUrl.lastIndexOf('/') + 1);
                var hostGpuDevKitName = versionMap[key][INDEX_GPU_DEV_KIT_NAME];
                var hostGpuDevKitURL = versionMap[key][INDEX_GPU_DEV_KIT_URL];
                var hostGpuDevKitText = 'The ' + hostGpuDevKitName + ' is available as a separate download <a href="' + hostGpuDevKitURL + '">here</a>.<br/><br/>';
                var hostGsgName = versionMap[key][INDEX_GSG_NAME];
                var hostGsgURL = versionMap[key][INDEX_GSG_URL];
                var hostQsgName = versionMap[key][INDEX_QSG_NAME];
                var hostQsgURL = versionMap[key][INDEX_QSG_URL];
                
                var config = hostOS + ' ' + hostArch;
                if (hostDistro.length > 0)
                    config += ' ' + hostDistro;
                config += ' ' + hostVersion;
                
                // title
                {
                    var hostInstallerTitle = hostOS;
                    
                    if (hostDistro.length > 0)
                        hostInstallerTitle += ' ' + hostDistro;
                        
                    hostInstallerTitle += ' ' + hostVersion + ' ' + hostArch;
    
                    $('#titleHostInstaller').html('Download Host Installer for ' + hostInstallerTitle);                
                }
                
                $('#hostInstallerFile').html(hostFileName);
                $('#hostInstallerSize').html(hostInstallerSize);
                $('#hostInstallerConfig').html(config);
                $('#hostMd5Checksum').html(hostMd5String);
                $('#hostDownloadButtonText').html('Download (' + hostInstallerSize + ')');
                $('#hostDownloadButtonHref').attr('href', hostUrl);
                $('#hostGSG').attr('href', hostGsgURL);
                $('#hostGSG').html(hostGsgName);
                $('#hostQSG').attr('href', hostQsgURL);
                $('#hostQSG').html(hostQsgName);
                $('#downloadPanelTitle').html('Download Installers');

                if (hostGpuDevKitURL.length > 0)
                    $('#hostGpuDevKitText').html(hostGpuDevKitText);
                else
                    $('#hostGpuDevKitText').empty();

                $('#panelHostInstaller').show();
            }
        }
    }

    function showInstructionsPanels()
    {
        showTargetInstructionsPanel();
        showHostInstructionsPanel();
    }
    
    function showTargetInstructionsPanel()
    {
        var key = targetOS + '/' + targetArch + '/' + targetDistro + '/' + targetVersion + '/' + targetPackage;
        var entry = versionMap[key];
        
        if (entry == null)
            return;             // don't show target panel if target entries were not set
        
        var targetInstructions = versionMap[key][INDEX_INSTRUCTIONS];
        $('#targetInstructions').html(targetInstructions);
        $('#targetInstructionsPanel').show();
    }

    function showHostInstructionsPanel()
    {
        var key = hostOS + '/' + hostArch + '/' + hostDistro + '/' + hostVersion + '/' + hostPackage;
        var entry = versionMap[key];
        
        if (entry == null)
            return;             // don't show host panel if host entries were not set
        
        var hostInstructions = versionMap[key][INDEX_INSTRUCTIONS];
        $('#hostInstructions').html(hostInstructions);
        $('#hostInstructionsPanel').show();
    }

    function setupButtonHighlighting(selector)
    {
        $(selector).click(function(e)
        {
            $(selector).each(function(index)
            {
                var $button = $(this);
                $button.removeClass('cuda-btn-success');
                $button.addClass('btn-success');
            });

            var $button = $(this);
            $button.removeClass('btn-success');
            $button.addClass('cuda-btn-success');
        });
    }

    function clearTargetArch()
    {
        targetArch = '';
        $('#targetArchRow').hide();
        clearTargetDistro();
    }
    
    function clearHostArch()
    {
        hostArch = '';
        $('#hostArchRow').hide();
        clearHostDistro();
    }

    function clearTargetDistro()
    {
        targetDistro = '';
        $('#targetDistroRow').hide();
        clearTargetVersion();
    }

    function clearHostDistro()
    {
        hostDistro = '';
        $('#hostDistroRow').hide();
        clearHostVersion();
    }

    function clearTargetVersion()
    {
        targetVersion = '';
        $('#targetVersionRow').hide();
        clearTargetPackage();
    }

    function clearHostVersion()
    {
        hostVersion = '';
        $('#hostVersionRow').hide();
        clearHostPackage();
    }

    function clearTargetPackage()
    {
        targetPackage = '';
        $('#targetPackageRow').hide();
        clearCrossPrompt();
    }

    function clearHostPackage()
    {
        hostPackage = '';
        $('#hostPackageRow').hide();
        clearDownloadPanels();
    }

    function clearCrossPrompt()
    {
        $('.crossButton').each(function(index)
        {
            var $button = $(this);
            $button.removeClass('cuda-btn-success');
            $button.addClass('btn-success');
        });
        
        $('#crossPrompt').hide();
        clearHostPanel();
    }
    
    function clearHostPanel()
    {
        $('#selectHostPlatformPanel').hide();
        clearDownloadPanels();
        clearHostArch();
    }

    function clearDownloadPanels()
    {
        $('#downloadPanel').hide();
        clearInstructionsPanels();
    }

    function clearInstructionsPanels()
    {
        $('#targetInstructionsPanel').hide();
        $('#hostInstructionsPanel').hide();
    }

    function makeSelectorJqueryWorthy(selector)
    {
        selector = selector.replace(/\./g, '');      // remove dot
        selector = selector.replace(/\(/g, '');      // remove open paren
        selector = selector.replace(/\)/g, '');      // remove close paren
        selector = selector.replace(/\-/g, '');      // remove dash
        selector = selector.replace(/ /g, '');        // remove space
        return selector; 
    }
}
);
</script>

</div></div></div></div>


