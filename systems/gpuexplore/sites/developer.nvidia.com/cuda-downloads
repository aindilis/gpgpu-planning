var INDEX_GSG_NAME = 5;
var INDEX_GSG_URL = 6;
var INDEX_QSG_NAME = 7;
var INDEX_QSG_URL = 8;
var INDEX_INSTRUCTIONS = 9;

var announcement='';
var faq='<a href="http://developer.nvidia.com/cuda-downloads/faq">Frequently Asked Questions</a>';
var documentation='<a href="http://docs.nvidia.com/cuda/index.html">Online Documentation</a>';
